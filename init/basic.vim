syntax on
set background=dark
set tabstop=4
set shiftwidth=4
set autoindent
set showcmd
set nocompatible
set t_Co=256
set number
set laststatus=2
set hlsearch
set encoding=utf-8
set cursorcolumn
set cursorline
set autoindent
set cindent
set ruler
set display=lastline
set tags=tags;
set path+=~/.vim,.
highlight CursorLine   cterm=NONE ctermbg=darkgrey ctermfg=NONE guibg=NONE guifg=NONE
highlight CursorColumn cterm=NONE ctermbg=darkgrey ctermfg=NONE guibg=NONE guifg=NONE
